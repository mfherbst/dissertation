@Article{Bukowski07,
  author    = {Bukowski, Robert and Szalewicz, Krzysztof and Groenenboom, Gerrit C. and van der Avoird, Ad},
  title     = {Predictions of the Properties of Water from First Principles},
  journal   = {Science},
  year      = {2007},
  volume    = {315},
  number    = {5816},
  pages     = {1249-1252},
  abstract  = {A force field for water has been developed entirely from first principles, without any fitting to experimental data. It contains both pairwise and many-body interactions. This force field predicts the properties of the water dimer and of liquid water in excellent agreement with experiments, a previously elusive objective. Precise knowledge of the intermolecular interactions in water will facilitate a better understanding of this ubiquitous substance.},
